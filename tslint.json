{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": {
            "options": "single"
        },
        "max-classes-per-file": false,
        "variable-name": {
            "options": "allow-leading-underscore"
        },
        "interface-name": false,
        "prefer-for-of": false
    },
    "rulesDirectory": []
}
